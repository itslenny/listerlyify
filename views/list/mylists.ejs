<% include ../partials/header %>

<% include ../partials/alerts %>

<div class="header-space"></div>

<div class="row">
  <div class="col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2">
    <table id="myliststable" class="display compact" width="100%" cellspacing="0">
      <thead>
        <tr>
          <th>Name</th>
          <th>Description</th>
        </tr>
      </thead>

      <tbody>
        <% lists.forEach(function(list, idx, listsArr) { %>
          <tr>
            <td>
              <a href="/list/show/<%=list.id%>"><%=list.name%></a>
            </td>
            <td><%=list.description%></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>

<!-- Local JS -->
<script src="/js/mylists.js"></script>

<% include ../partials/footer %>

